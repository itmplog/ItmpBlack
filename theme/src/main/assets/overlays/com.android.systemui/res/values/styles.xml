<?xml version="1.0" encoding="utf-8"?>
<!--You are editing SystemUI's Styles-->

<resources>
    <style name="TextAppearance.Material.Notification.Parenthetical" parent="@android:style/TextAppearance.Material.Notification">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="systemui_theme" parent="@android:style/Theme.Material">
        <item name="android:colorControlActivated">@*common:color/midnight_theme</item>
        <item name="android:colorPrimary">@*common:color/midnight_primary</item>
    </style>
    <style name="systemui_theme_light" parent="@android:style/Theme.Material">
        <item name="android:colorControlActivated">@*common:color/midnight_theme</item>
        <item name="android:colorPrimary">@*common:color/midnight_primary</item>
    </style>
	<style name="Theme.SystemUI.Dialog" parent="@android:style/Theme.Material.Dialog">
        <item name="android:colorControlActivated">@*common:color/midnight_theme</item>
        <item name="android:colorPrimary">@*common:color/midnight_primary</item>
    </style>
    <style name="Theme.SystemUI.Dialog.Alert" parent="@android:style/Theme.Material.Dialog.Alert" />
</resources>
