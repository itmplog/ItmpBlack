        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Platform.AppCompat" parent="@*android:style/Theme.Material" />
    <style name="Base.V7.Theme.AppCompat" parent="@style/Platform.AppCompat" />
    <style name="ThemeOverlay.AppCompat" parent="@*android:style/Theme.Material" />
    <style name="ThemeOverlay.AppCompat.Light" parent="@*android:style/Theme.Material" />
    <style name="ThemeOverlay.AppCompat.Dark" parent="@*android:style/Theme.Material" />
    <style name="ThemeOverlay.AppCompat.ActionBar" parent="@*android:style/Theme.Material" />
    <style name="ThemeOverlay.AppCompat.Dark.ActionBar" parent="@*android:style/Theme.Material" />
